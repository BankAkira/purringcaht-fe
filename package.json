{
  "name": "social-bureau-frontend",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "start:trydev": "vite --mode trydev",
    "start:prod": "vite --mode production",
    "build:prod": "tsc && vite build --mode production",
    "build:trydev": "tsc && vite build --mode trydev",
    "build": "tsc && vite build",
    "deploy:prod": "firebase deploy --only hosting:prod",
    "deploy:trydev": "npm run build:trydev && firebase deploy --only hosting:trydev",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "prettier:check": "npx prettier --check .",
    "prettier:write": "npx prettier --write .",
    "preview": "vite preview --port 8080",
    "sc:jutc": "typechain --target ethers-v5 --out-dir src/contract/jutc src/abis/JUTC.json",
    "sc:market-antenna": "typechain --target ethers-v5 --out-dir src/contract/market-antenna src/abis/MarketAntenna.json",
    "sc:fountain": "typechain --target ethers-v5 --out-dir src/contract/fountain src/abis/Fountain.json",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal public/favicon.svg",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "rm -rf dist && npm run prettier:check && npm run lint"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
